    <script type="text/javascript">

        const anio_humano = 7
        let num = prompt("Ingrese la edad de la mascota");
        Calcular_Edad_Canina(num);

        function Calcular_Edad_Canina(edad)
        {
            let conversion = anio_humano*edad;
            alert(`Su mascota tiene ${conversion} a√±os en edad canina`);

        }

    </script>
